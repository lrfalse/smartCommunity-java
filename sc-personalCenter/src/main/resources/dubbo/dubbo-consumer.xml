<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application  name="dubbo-consumer" owner="dubbo-consumer"/>
    <dubbo:registry protocol="zookeeper" address="${zookeeper.register.address}" client="zkclient" />


    <dubbo:reference id="districtService" interface="com.commons.service.DistrictService" />
    <dubbo:reference id="userService" interface="com.commons.service.UserService" />

    <!--<dubbo:annotation package="com.commons.service"/>-->

</beans>